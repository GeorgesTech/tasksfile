{
  "extends": [
      "tslint:latest", "tslint-config-prettier"
  ],
  "rules": {
      "prettier": true,
      "max-classes-per-file": false,
      "no-console": false,
      "no-empty": false,
      "no-shadowed-variable": false,
      "no-submodule-imports": false
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}